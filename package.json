    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.28.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.19.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
